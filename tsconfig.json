{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "commonjs",
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "target": "ES6",
    "outDir": "./dist",
    "skipLibCheck": true,
  },
  "lib": "ES2023",
  "include": [
    "./package.json",
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
}